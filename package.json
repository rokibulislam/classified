{
  "name": "classified",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.dev.config.js",
  "scripts": {
    "start": "run-s build && cross-env-shell \"NODE_ENV=$NODE_ENV\" node dist/src/app.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.prod.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "lint": "eslint . --ext .ts,.js",
    "fix:lint": "eslint . --ext .ts,.js --fix",
    "test": "cross-env NODE_ENV=test node node_modules/jest/bin/jest --runInBand",
    "test:watch": "cross-env NODE_ENV=test node node_modules/jest/bin/jest --watch --runInBand",
    "test:cover": "cross-env NODE_ENV=test jest --coverage=true --runInBand",
    "clean": "rimraf dist"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint . --ext .ts,.js --fix",
      "prettier \"src/**/*.ts\" --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run build"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@elastic/elasticsearch": "^7.11.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/mongoose": "^5.10.3",
    "amqplib": "^0.7.0",
    "apollo-server-express": "^2.19.2",
    "aws-sdk": "^2.853.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "elasticsearch": "^16.7.2",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "graphql": "^15.5.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-resolvers": "^0.4.2",
    "graphql-tools": "^7.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.14",
    "nodemailer": "^6.5.0",
    "nodemon": "^2.0.7",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3",
    "yup": "^0.32.9"
